<div class="gallery">
	<div class="container">
		<!--content-top-->
		<div class="content-top">
			<h3>Mis albumes</h3>
		</div>
		<!--//content-top-->
		<!--content-mid-->
		
		<div class="row">
			<% @songs.each do |song| %>
