<p id="notice"><%= notice %></p>

<h1>Listing Song Lists</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Hour</th>
      <th>Client</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @song_lists.each do |song_list| %>
      <tr>
        <td><%= song_list.date %></td>
        <td><%= song_list.hour %></td>
        <td><%= song_list.client %></td>
        <td><%= link_to 'Show', song_list %></td>
        <td><%= link_to 'Edit', edit_song_list_path(song_list) %></td>
        <td><%= link_to 'Destroy', song_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Song list', new_song_list_path %>
